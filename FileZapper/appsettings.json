{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Debug" },
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.txt",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "WithEnvironmentUserName", "WithProcessId", "WithThreadId" ]
  }
}